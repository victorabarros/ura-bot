  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.16.0",
    "@types/axios": "0.14.0",
    "@types/express": "4.17.13",
    "@types/jest": "^27.0.3",
    "@types/node": "16.11.7",
    "@types/pg": "^8.6.1",
    "@types/sequelize": "^4.28.10",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.3",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "sequelize-cli": "^6.3.0",
    "ts-node-dev": "1.1.8",
    "typescript": "4.4.4"
  },
  "dependencies": {
    "axios": "0.24.0",
    "express": "4.17.1",
    "http-status": "1.5.0",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.12.0-beta.3",
    "twitter-lite": "1.1.0"
  }
